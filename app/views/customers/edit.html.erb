<h1>登録情報編集</h1>
<%= form_with model: @customer do |f| %>
<table>
 <tbody>
  <tr>
   <th>氏名</th>
   <td><%= f.text_field :last_name %> <%= f.text_field :first_name %></td>
  </tr>
  <tr>
   <th>カナ</th>
   <td><%= f.text_field :last_name_kana %> <%= f.text_field :first_name_kana %></td>
  </tr>
  <tr>
   <th>郵便番号</th>
   <td><%= f.text_field :postal_code %></td>
  </tr>
  <tr>
   <th>住所</th>
   <td><%= f.text_field :address %></td>
  </tr>
  <tr>
   <th>電話番号</th>
   <td><%= f.text_field :telephone_number %></td>
  </tr>
  <tr>
   <th>メールアドレス</th>
   <td><%= f.text_area :email %></td>
  </tr>
 </tbody>
</table>
<%= f.submit '保存' %>

<p></p>
<%= link_to '退会', customers_unsubscribe_path %>

<% end %>